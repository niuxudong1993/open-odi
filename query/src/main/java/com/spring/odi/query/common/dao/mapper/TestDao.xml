<?xml version="1.0" encoding="UTF-8"?>
        <!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.spring.odi.query.common.dao.TestDao">
<update id="testMethod" parameterType="String">
            update test set id = 0
    </update>
    <update id="testQueue" parameterType="String">
            update test set id = #{test}
    </update>
    <select id="selectQueue" resultType="String">
            select id from test
    </select>
</mapper>